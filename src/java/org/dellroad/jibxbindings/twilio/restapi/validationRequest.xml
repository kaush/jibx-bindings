<binding package="org.dellroad.jibxbindings.twilio.restapi">
    <mapping name="ValidationRequest" class="org.dellroad.jibxbindings.twilio.restapi.ValidationRequest" ordered="false" flexible="true">
        <value name="AccountSid" get-method="getAccountSid" set-method="setAccountSid"/>
        <value name="PhoneNumber" get-method="getPhoneNumber" set-method="setPhoneNumber"/>
        <value name="FriendlyName" get-method="getFriendlyName" set-method="setFriendlyName"/>
        <value name="ValidationCode" get-method="getValidationCode" set-method="setValidationCode"/>
    </mapping>
</binding>
